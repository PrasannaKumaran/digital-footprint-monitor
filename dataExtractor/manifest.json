{
    "manifest_version": 3,
    "name": "dataExtractor",
    "version": "1.0",
    "permissions": [
        "tabs",
        "history",
        "browsingData",
        "bookmarks",
        "identity.email",
        "identity",
        "alarms",
        "downloads"
    ],
    "oauth2": {
        "client_id": "<ENTER YOUR GOOGLE CLIENT ID HERE>",
        "scopes": ["profile", "email"]
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    }
}
